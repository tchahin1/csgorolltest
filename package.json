{
  "author": "Tech board @ Ankora",
  "name": "ankora-nx-setup",
  "version": "0.0.2",
  "license": "MIT",
  "scripts": {
    "build:all:a": "nx run-many --target=build --all --configuration=a",
    "build:all:d": "nx run-many --target=build --all --configuration=d",
    "build:all:ci": "nx run-many --target=build --all --configuration=ci",
    "build:all:t": "nx run-many --target=build --all --configuration=t",
    "build:all:p": "nx run-many --target=build --all --configuration=p",
    "dev:app": "nx serve app",
    "dev:api": "nx serve api --verbose --configuration=d",
    "db:reset": "npx nx run models:db-reset",
    "db:reset:t": "npx env-cmd -f .env.test nx run models:db-reset --configuration=t",
    "db:migrate:generate": "npx nx run models:migrate-generate",
    "db:migrate": "npx nx run models:migrate-up",
    "db:client:generate": "npx nx run models:gen-client",
    "db:studio": "npx nx run models:studio",
    "db:seed": "npx nx run models:seed",
    "db:reset:dev": "npx prisma migrate reset",
    "db:migrate:deploy": "prisma migrate deploy",
    "api:setup:module": "nx workspace-generator nest-module-generator --name=$MODULE",
    "prepare": "npx husky install",
    "lint": "nx run-many --target=lint --all --output-style=static",
    "prettier:check": "nx format:check --all",
    "prettier:fix": "nx format:write --all",
    "api:client:generate": "npx nx run api-client:generate && npm run prettier:fix",
    "build:storybook": "npx nx build-storybook ui-library",
    "storybook": "npx nx storybook ui-library",
    "export:app": "npx nx run app:export",
    "cypress": "npx nx e2e app-e2e --watch",
    "prepare:all": "npm install && npm run db:client:generate && npm run db:reset",
    "test": "npm run db:reset:t && npx nx run api:test --configuration=test --runInBand"
  },
  "private": true,
